<?xml version="1.0" encoding="UTF-8"?>

<templates>

    <t t-name="GetExternalSchema.open_wizard_button" owl="1">
        <button type="button" class="btn btn-secondary" t-on-click="onClickGetExternalSchema">
            Get External Schema
        </button>
    </t>

    <t t-name="GetExternalSchema.buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="primary" owl="1">
        <xpath expr="//t[contains(@t-if, 'isExportEnable')]" position="before">
            <t t-call="GetExternalSchema.open_wizard_button"/>
        </xpath>
    </t>

</templates>
