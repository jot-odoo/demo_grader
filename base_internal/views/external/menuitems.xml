<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="external_ir_model_action" model="ir.actions.act_window">
        <field name="name">Models</field>
        <field name="res_model">external.ir.model</field>
        <field name="view_mode">tree</field>
