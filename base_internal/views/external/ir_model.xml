<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <record id="external_ir_model_view_tree" model="ir.ui.view">
        <field name="name">external.ir.model.view.tree</field>
        <field name="model">external.ir.model</field>
        <field name="arch" type="xml">
            <tree string="External Models" editable="top" js_class="get_external_schema_tree">
                <field name="name"/>
                <field name="technical_name"/>
                <field name="version_ids" widget="many2many_tags"/>
            </tree>
        </field>
    </record>
    
    <record id="external_ir_model_fields_view_tree" model="ir.ui.view">
        <field name="name">external.ir.model.fields.view.tree</field>
        <field name="model">external.ir.model.fields</field>
        <field name="arch" type="xml">
            <tree string="External Fields" editable="top" js_class="get_external_schema_tree">
                <field name="model_id"/>
                <field name="name"/>
                <field name="technical_name"/>
                <field name="ttype"/>
                <field name="version_ids" widget="many2many_tags"/>
            </tree>
        </field>
    </record>

    <record id="external_ir_model_version_view_tree" model="ir.ui.view">
        <field name="name">external.ir.model.version.view.tree</field>
        <field name="model">external.ir.model.version</field>
        <field name="arch" type="xml">
            <tree string="Database Versions" editable="top">
                <field name="name"/>
            </tree>
        </field>
    </record>

</odoo>